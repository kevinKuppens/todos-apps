{
    "compilerOptions": {
        "target": "ES2020",
      "module": "CommonJS",
      "lib":["ES2020"],
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "removeComments": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "**/**/node_modules",
        "src/migration/**/**",
        "node_modules"
      ]
}